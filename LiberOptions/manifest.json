{
  "manifest_version": 2,
  "name": "LiberOptions Enhanced",
  "version": "0.1.1",
  "description": "Add automatic bet sizing and keyboard controls to LiberOptions.",
  "permissions": [
    "*://*.liberoptions.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.liberoptions.com/*"],
      "js": ["js/jquery-2.1.3.min.js", "js/extension.js"],
      "run_at": "document_end"
    }
  ]
}
